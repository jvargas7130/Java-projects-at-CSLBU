		System.out.println("y" + y);
		
 		
 		
 		int [][] array = {{0, 1, 2, 3, 4},
 							{1,6,7,8,9},
 							{2,11,12,13},
 							{3,15,16,17},
 							{4,19,20,21}};  	 	
 		boolean testInput = true;  	 	
 		//while ( testInput ) { 
 	 	 	
 	 	 	try{  	 	 	 	
 	 	 		//int input = in.nextInt(); //exceptions?  	 	 	 	
 	 	 		System.out.println(array[x][y]);  	 	 	 	
 	 	 		
 	 	 	
 	 	 	}catch (ArrayIndexOutOfBoundsException ob){
