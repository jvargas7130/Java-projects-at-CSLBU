import javax.swing.*;
import java.awt.*;
import java.awt.event.*;


public class GuiApp extends JFrame {
	
	private Panel panel;
	
	public GuiApp(){
		setBounds(100, 100, 1900, 1018); //x,y,w,h
		panel = new Panel();
		getContentPane().add(panel);
	}
	
	
	public static void main(String [] args){
		GuiApp f = new GuiApp();
		f.setTitle("Drawing Demo");
		f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		f.setVisible(true);
	}
	
	

}
